      <command>git clean -x -d -f
${WORKSPACE}/deploy/trigger.sh --color --dockerpull --test --test_format=log --release --releasedir=/mdsplus/release --publishdir=/mdsplus/dist --keys=/mdsplus/certs/jenkins</command>
    </hudson.tasks.Shell>
    <hudson.plugins.parameterizedtrigger.TriggerBuilder plugin="parameterized-trigger@2.32">
