    </com.coravy.hudson.plugins.github.GithubProjectProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@3.0.5">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <refspec>+refs/heads/alpha:refs/remotes/origin/alpha</refspec>
        <url>git@github.com:/MDSplus/mdsplus.git</url>
        <credentialsId>35126f42-ed07-43bc-8659-ba2b1484f602</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
